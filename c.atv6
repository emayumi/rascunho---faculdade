#include <stdio.h>
int main(int argc, char* argv[]) 
{
    int M[3] [3], i, j;
    for (i=0; i<3; i++) {
        for (j=0; j<3; j++) {
            if (i ==j) M[i] [j]=1;
            else M[i] [j]=0;
        }
    }
        printf("Mostrando o conteudo da Matriz\n");
        for (i=0; i<3; i++) {
            for (j=0; j<3; j++) {
                printf("%d\t", M[i] [j]);
            }
                printf("\n");
        }
        return 0;
}
