#include <stdio.h>
#include <stdlib.h>
int main (int agrc, char* argv[])

{ int V_lido[5], i, maior, posicao = 0;
  for (i=0; i<=4; i++) {
      printf("Digite V[%d] = ", i);
      scanf("%d", &V_lido[i]);
  }
  maior = V_lido[0];
  for (i=0; i<=4; i++) {
      if (V_lido[i] > maior) {
          maior = V_lido[i];
          posicao = i;
      }
  }
  printf("\nMaior valor: %d\n", maior);
  printf("Posição: %d\n", posicao);
  
  return 0;
}
